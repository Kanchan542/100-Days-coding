//Given 2 integer arrays X and Y of same size. Consider both arrays as vectors and
// print the sum of maximum scalar product (Dot product) of 2 vectors.
import java.util.*;
class TallentBettle{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the size of array: ");
        int n=sc.nextInt();
        System.out.println("Enter the element first array: ");
        int arr1[]=new int[n];
        for(int i=0;i<n;i++){
            arr1[i]=sc.nextInt();
        }
        System.out.println("Enter the element of second array: ");
        int arr2[]=new int[n];
        for(int j=0;j<n;j++){
            arr2[j]=sc.nextInt();
        }
        int result=0;
        for(int i=0;i<n;i++){
            
               int prod= arr1[i]*arr2[i];
			   result=prod+result;
            
        }
        System.out.println(result);
    }
        
}
