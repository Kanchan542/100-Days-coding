//Get a number from the user and check whether the given number is odd or even 

import java.util.*;
class TallentBettle{
	public static void main(String args[]){
		Scanner sc=new Scanner(System.in);
		int a;
		a=sc.nextInt();
		
		if(a%2==0){
			System.out.println("Even");
		}
		else{
			System.out.println("Odd");
		}
	}
}
